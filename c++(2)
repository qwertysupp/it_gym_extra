#include <iostream>

using namespace std;

int exponentiation() {
	int number,degree;
	cin >> number;
	cin >> degree;
	int z = 0;
	for (int i = degree;i == 0;i--) {
		z = number * number;
		number == z;
	}
	cout << z;
	return z;
}
int main() {
	exponentiation();
}
